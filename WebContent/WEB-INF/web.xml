<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>Servlet_Project</display-name>
  <servlet>
    <servlet-name>add</servlet-name>
    <servlet-class>com.project.AddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>add</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>square</servlet-name>
    <servlet-class>com.project.SquareServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>square</servlet-name>
    <url-pattern>/square</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.project.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>home</servlet-name>
    <servlet-class>com.project.HomeServlet</servlet-class>
    <init-param>
      <param-name>CompanyName</param-name>
      <param-value>Google</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>com.project.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>landing</servlet-name>
    <servlet-class>com.project.LandingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>landing</servlet-name>
    <url-pattern>/landing</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>CompanyName</param-name>
    <param-value>ADP</param-value>
  </context-param>
  <servlet>
    <servlet-name>student</servlet-name>
    <servlet-class>com.project.StudentServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>student</servlet-name>
    <url-pattern>/registerStudent</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>StudentFilter</display-name>
    <filter-name>StudentFilter</filter-name>
    <filter-class>com.project.StudentFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>StudentFilter</filter-name>
    <url-pattern>/registerStudent</url-pattern>
  </filter-mapping>
 <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.project.login.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>com.project.login.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/Logout</url-pattern>
  </servlet-mapping>
</web-app>